{
  "version": 2,
  "builds": [
    {
      "src": "wikicomic_vercel_ready/api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "wikicomic_vercel_ready/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "wikicomic_vercel_ready/api/index.py"
    }
  ],
  "functions": {
    "wikicomic_vercel_ready/api/index.py": {
      "maxDuration": 300
    }
  },
  "env": {
    "PYTHONPATH": "wikicomic_vercel_ready"
  }
} 